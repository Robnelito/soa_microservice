generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Clients {
    idClient          String @id @default(uuid())
    firstnameClient   String
    lastnameClient    String
    mailClient        String @unique
    phoneNumberClient String
    addressClient     String
    remnantsClient    Float
    accountNumberClient String @unique
}
