generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  user_id        String  @id @default(uuid())
  user_firstname String
  user_lastname  String?
  user_email     String  @unique
  user_password  String
}